{"components/common/footer":"{{#if banners.bottom}}\n<div class=\"banners\" data-banner-location=\"bottom\">\n    <div class=\"banner\">\n        {{{limit banners.bottom 1}}}\n    </div>\n</div>\n{{/if}}\n\n\n<div class=\"subscribe\">\n\t<div class=\"container\">\n\t\t\n\t<div class=\"col-sm-12 col-md-6 col-lg-6 footer-socials\">\n\t\t{{> components/common/social-links}}\n\t</div>\n\t\t\n\t <div class=\"col-sm-12 col-md-6 col-lg-6 subscription-form\">\t\n\t {{#if settings.show_newsletter_box}}\n        {{> components/common/subscription-form}}\n     {{/if}}\n\t </div>\n\t</div>\n</div>\n\n<footer class=\"footer\" role=\"contentinfo\">\n    <div class=\"container\">\n        <section class=\"footer-info\">\n            \n            <article class=\"footer-info-col col-sm-12 col-md-2 col-lg-2\" data-section-type=\"footer-webPages\">\n                <h5 class=\"footer-info-heading\">{{lang 'footer.navigate'}}</h5>\n                <ul class=\"footer-info-list\">\n                    {{#each pages}}\n                    <li>\n                        <a href=\"{{url}}\">{{name}}</a>\n                    </li>\n                    {{/each}}\n                    <li>\n                        <a href=\"{{urls.sitemap}}\">{{lang 'sitemap.page_title'}}</a>\n                    </li>\n                </ul>\n            </article>\n\n            <article class=\"footer-info-col col-sm-12 col-md-2 col-lg-2\" data-section-type=\"footer-categories\">\n                <h5 class=\"footer-info-heading\">{{lang 'footer.categories'}}</h5>\n                <ul class=\"footer-info-list\">\n                    {{#each categories}}\n                        <li>\n                            <a href=\"{{url}}\">{{name}}</a>\n                        </li>\n                    {{/each}}\n                </ul>\n            </article>\n\n            <article class=\"footer-info-col col-sm-12 col-md-2 col-lg-2\" data-section-type=\"footer-brands\">\n                <h5 class=\"footer-info-heading\">{{lang 'footer.brands'}}</h5>\n                <ul class=\"footer-info-list\">\n                    {{#each shop_by_brand}}\n                        <li>\n                            <a href=\"{{url}}\">{{name}}</a>\n                        </li>\n                    {{/each}}\n                    <li><a href=\"{{urls.brands}}\">{{lang 'common.view_all'}}</a></li>\n                </ul>\n            </article>\n\n\t\t\t<article class=\"footer-info-col col-sm-12 col-md-2 col-lg-2\" data-section-type=\"newsletterSubscription\">\n\t            <h5 class=\"footer-info-heading\">Latest News</h5>   \n\t\t\t\t\n\t\t\t\t<div class=\"blog-section\"></div>\t\n\t\t\t\t\n\t\t\t\t\n                \n                {{> components/common/payment-icons}}\n            </article>\n\n\n            <article class=\"footer-info-col col-sm-12 col-md-4 col-lg-4 store-info\" data-section-type=\"storeInfo\">\n                <!--<h5 class=\"footer-info-heading\">{{lang 'footer.info'}}</h5>-->\n               \n               \t<img src=\"{{cdn '/assets/img/footer-logo.png'}}\" class=\"footer-logo\" />\n\t\t\t   \t<p class=\"slogan\">A Revolution In Custom Gauges</p>\n                <address>{{nl2br settings.address}}</address>\n                {{#if settings.phone_number}}\n                    <strong>{{lang 'footer.call_us' phone_number=settings.phone_number}}</strong>\n                {{/if}}\n            </article>\n\n\n                   </section>\n    </div>\n</footer>\n\n{{#if theme_settings.show_powered_by}}\n    <div class=\"footer-copyright\">\n        <p class=\"powered-by\">Copyright &copy; Speedhut. All rights reserved. </p>\n    </div>\n{{/if}}\n","components/common/social-links":"{{#if social_media}}\n    <ul class=\"socialLinks socialLinks--alt\">\n        <li class=\"text\">Stay Connected:</li>\n        {{#each social_media}}\n            <li class=\"socialLinks-item\">\n                {{#if display_name '===' 'google+'}}\n                    <a class=\"icon icon--google\" href=\"{{url}}\" target=\"_blank\">\n                        <svg><use xlink:href=\"#icon-google\" /></svg>\n                    </a>\n                {{else}}\n                    <a class=\"icon icon--{{display_name}}\" href=\"{{url}}\" target=\"_blank\">\n                        <svg><use xlink:href=\"#icon-{{display_name}}\" /></svg>\n                    </a>\n                {{/if}}\n            </li>\n        {{/each}}\n    </ul>\n{{/if}}\n","components/common/payment-icons":"<div class=\"footer-payment-icons\">\n    {{#if theme_settings.show_accept_amex}}\n        <svg class=\"footer-payment-icon\"><use xlink:href=\"#icon-logo-american-express\"></use></svg>\n    {{/if}}\n    {{#if theme_settings.show_accept_discover}}\n        <svg class=\"footer-payment-icon\"><use xlink:href=\"#icon-logo-discover\"></use></svg>\n    {{/if}}\n    {{#if theme_settings.show_accept_mastercard}}\n        <svg class=\"footer-payment-icon\"><use xlink:href=\"#icon-logo-mastercard\"></use></svg>\n    {{/if}}\n    {{#if theme_settings.show_accept_paypal}}\n        <svg class=\"footer-payment-icon\"><use xlink:href=\"#icon-logo-paypal\"></use></svg>\n    {{/if}}\n    {{#if theme_settings.show_accept_visa}}\n        <svg class=\"footer-payment-icon\"><use xlink:href=\"#icon-logo-visa\"></use></svg>\n    {{/if}}\n</div>\n","components/common/subscription-form":"<h5 class=\"footer-info-heading\">{{lang 'newsletter.subscribe'}}</h5>\n<!--<p>{{lang 'newsletter.subscribe_intro'}}</p>-->\n\n<form class=\"form\" action=\"{{urls.subscribe.action}}\" method=\"post\">\n    <fieldset class=\"form-fieldset\">\n        <input type=\"hidden\" name=\"action\" value=\"subscribe\">\n        <input type=\"hidden\" name=\"nl_first_name\" value=\"bc\">\n        <input type=\"hidden\" name=\"check\" value=\"1\">\n        <div class=\"form-field\">\n            <label class=\"form-label is-srOnly\" for=\"nl_email\">{{lang 'common.email_address'}}</label>\n            <div class=\"form-prefixPostfix wrap\">\n                <input class=\"form-input\" id=\"nl_email\" name=\"nl_email\" type=\"email\" value=\"{{customer.email}}\" placeholder=\"{{lang 'newsletter.email_placeholder'}}\">\n                <input class=\"button button--primary form-prefixPostfix-button--postfix\" type=\"submit\" value=\"{{lang 'newsletter.subscribe_submit'}}\">\n            </div>\n        </div>\n    </fieldset>\n</form>\n"}
